{
  "name": "firebase-and-react",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(scss|css)$": "<rootDir>/__mocks__/styleMock.js",
      "^:(.*)$": "<rootDir>/src/$1",
      "^Api(.*)$": "<rootDir>/src/Api/$1",
      "^Views(.*)$": "<rootDir>/src/Views/$1",
      "^State(.*)$": "<rootDir>/src/State/$1",
      "^Constants(.*)$": "<rootDir>/src/Constants/$1",
      "^Components(.*)$": "<rootDir>/src/Components/$1",
      "^Utils(.*)$": "<rootDir>/src/Utils/$1"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "firebase": "^5.5.5",
    "history": "^4.7.2",
    "prop-types": "^15.6.2",
    "query-string": "5.0.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1"
  },
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.dev",
    "build": "webpack --define process.env.NODE_ENV='\"production\"' --config config/webpack.config.prod",
    "deploy": "firebase deploy",
    "bd": "npm run lint && npm run build && npm run deploy",
    "lint": "eslint --ignore-path .gitignore .",
    "test": "jest"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^22.4.4",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^4.19.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "extract-css-chunks-webpack-plugin": "^3.2.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.4",
    "node-sass": "^4.9.4",
    "normalize.css": "^7.0.0",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.6",
    "react-hot-loader": "^3.1.3",
    "redux-devtools-extension": "^2.13.5",
    "resolve-url-loader": "^2.3.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^4.22.0",
