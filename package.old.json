{
  "name": "firebase-and-react",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(scss|css)$": "<rootDir>/__mocks__/styleMock.js",
      "^:(.*)$": "<rootDir>/src/$1",
      "^Api(.*)$": "<rootDir>/src/Api/$1",
      "^Views(.*)$": "<rootDir>/src/Views/$1",
      "^State(.*)$": "<rootDir>/src/State/$1",
      "^Constants(.*)$": "<rootDir>/src/Constants/$1",
      "^Components(.*)$": "<rootDir>/src/Components/$1",
      "^Utils(.*)$": "<rootDir>/src/Utils/$1"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "firebase": "4.6.2",
    "history": "^4.7.2",
    "prop-types": "^15.6.1",
    "query-string": "5.0.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.dev",
    "build": "webpack --define process.env.NODE_ENV='\"production\"' --config config/webpack.config.prod",
    "deploy": "firebase deploy",
    "bd": "npm run lint && npm run build && npm run deploy",
    "lint": "eslint --ignore-path .gitignore .",
    "test": "jest"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^22.4.3",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.4.3",
    "node-sass": "^4.7.2",
    "normalize.css": "^7.0.0",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.9",
    "react-hot-loader": "^3.1.3",
    "redux-devtools-extension": "^2.13.2",
    "resolve-url-loader": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-config": "^7.5.0",
    "webpack-dev-server": "^2.9.5"
  }
}
